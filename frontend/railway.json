{
  "$schema": "https://raw.githubusercontent.com/railwayapp/railway/master/schema.json",
  "build": {
    "builder": "NIXPACKS",
    "buildCommand": "npm install && npm run build"
  },
  "deploy": {
    "numReplicas": 1,
    "startCommand": "npm start",
    "healthcheckPath": "/",
    "healthcheckTimeout": 30
  }
}
